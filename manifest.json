{
    "manifest_version": 2,
    "name": "Quick Launch ", 
    "description": "Quick Launch by Regi",
    "version": "1.0.0",
    "icons": {
        "128": "images/logo128.png",
        "48": "images/logo48.png",
        "16": "images/logo16.png"
    },
    "browser_action": {
        "default_icon": "images/logo16.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}